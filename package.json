{
  "name": "visilant_visual_acuity_app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "start:prod": "expo start --no-dev",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --no-install",
    "build:bundle": "react-native build-android --mode=release",
    "build:clean": "rm ./android/app/build/outputs/apk/release/app-release.apks && rm ./android/app/build/outputs/apk/release/toc.pb && rm ./android/app/build/outputs/apk/release/universal.apk",
    "build:apks": "bundletool build-apks --bundle=./android/app/build/outputs/bundle/release/app-release.aab --output=./android/app/build/outputs/apk/release/app-release.apks --mode=universal --ks=./release.keystore --ks-key-alias=android",
    "build:apk": "unzip ./android/app/build/outputs/apk/release/app-release.apks -d ./android/app/build/outputs/apk/release/",
    "build:prod": "NODE_ENV=production yarn prebuild && yarn build:bundle && yarn build:clean && yarn build:apks && yarn build:apk",
    "ts:check": "tsc",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:fix": "eslint --fix --ext .tsx --ext .ts src/",
    "format": "prettier --check ./src",
    "postinstall": "husky install",
    "test": "jest",
    "typecheck": "tsc",
    "install:clean": "rm -rf node_modules && yarn install"
  },
  "dependencies": {
    "@expo-google-fonts/libre-franklin": "^0.2.3",
    "@react-native-community/netinfo": "9.3.10",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react": "~18.2.14",
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "expo": "~49.0.8",
    "expo-background-fetch": "~11.3.0",
    "expo-brightness": "~11.4.0",
    "expo-build-properties": "~0.8.3",
    "expo-file-system": "~15.4.5",
    "expo-font": "~11.4.0",
    "expo-haptics": "~12.4.0",
    "expo-localization": "~14.3.0",
    "expo-navigation-bar": "~2.3.0",
    "expo-screen-orientation": "~6.0.6",
    "expo-secure-store": "~12.3.1",
    "expo-sensors": "~12.3.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "i18next": "^23.4.6",
    "jwt-decode": "3.1.2",
    "react": "18.2.0",
    "react-i18next": "^13.2.0",
    "react-native": "0.72.10",
    "react-native-device-info": "^10.11.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-offline": "^6.0.2",
    "react-native-pager-view": "6.2.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-uuid": "^2.0.1",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-filesystem": "^2.0.0",
    "redux-persist-expo-securestore": "^2.0.0",
    "redux-thunk": "^2.4.2",
    "styled-components": "^6.0.7",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.0",
    "@types/jest": "^29.5.5",
    "@types/react-redux": "^7.1.26",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.47.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "prettier": "^3.0.2",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.1"
  },
  "resolutions": {
    "styled-components": "^6"
  },
  "private": true
}
